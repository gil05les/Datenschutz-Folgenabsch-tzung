{
  "version": 2,
  "buildCommand": "make build",
  "outputDirectory": "server/client-dist",
  "installCommand": "make install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

